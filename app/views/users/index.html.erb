<h1>Listing users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Location</th>

  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.user_name, user %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.location %></td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
