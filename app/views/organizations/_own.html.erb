<%= form_for(@organization.owners.build(:organization_id => @organization.id, :user_id => params[:user_id]), :remote => :true) do |f| %>
	<% users = User.all %>
	<% options = users.map { |u| [u.user_name, u.id] } %>

	<div><%= f.select(:user_id, options) %></div>
	<div><%= f.hidden_field :organization_id %></div>
	<p><%= f.submit "Add Admin!" %></p>
<% end %>