<%= form_for(@org.owners.find_by_user_id(params[:user_id]), html: {:method => :delete}, :remote => :true) do |f| %>
	<% users = @org.owners %>
	<% options = users.map { |u| [u.user_name, u.id] } %>

	<div><%= f.select(:user_id, options) %></div>
	<div><%= f.hidden_field :organization_id %></div>
  	<%= f.submit "Remove admin" %>
<% end %>
